import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        int[] arr= {1,5,78,30,12};
        int min=arr[0];
        int max=arr[0];
        for(int i=0;i<arr.length;i++){
            if(arr[i]<min){
                min=arr[i];
            }
            if(arr[i]>max){
                max=arr[i];
            }
        }
        System.out.println("min value:" +min);
        System.out.println("max value:" +max);
    }
}
